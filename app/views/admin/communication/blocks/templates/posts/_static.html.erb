<% if block.template_class.category %>
      category: <%= block.template_class.category.slug %>
<% end %>
      posts:
    <% block.template_class.posts.each do |post|
      id = post['id']
      unless id.blank?
        post = block.about&.website.posts.find id
        if post %>
        - <%= post.slug %>
        <% end %>
      <% end %>
    <% end %>
