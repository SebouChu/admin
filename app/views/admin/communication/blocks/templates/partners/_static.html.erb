<% if block.data.has_key?('elements') %>
<% block.data['elements'].each do |partner| %>
      - name: >
          <%= prepare_text_for_static partner['name'] %>
        url: >-
          <%= prepare_text_for_static partner['url'] %>
        logo: "<%= prepare_media_for_static partner, 'logo' %>"
<% end %>
<% end %>
