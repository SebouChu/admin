<% block.template.partners.each do |partner|
      if partner.organization %>
      - slug: "<%= partner.organization.slug %>"
      <% else %>
      - name: >
          <%= prepare_text_for_static partner['name'] %>
        url: >-
          <%= prepare_text_for_static partner['url'] %>
        logo: "<%= partner.blob&.id %>"
      <% end %>
<% end %>
