---
title: "<%= @post.title %>"
date: <%= @post.published_at %> UTC
slug: "<%= @post.slug %>"
<% if @post.featured_image.attached? %>
image: "<%= url_for @post.featured_image %>"
<% end %>
description: >
  <%= prepare_for_github @post.description %>
text: >
  <%= prepare_for_github @post.text %>
---
<%= @post.content_without_frontmatter.html_safe %>
