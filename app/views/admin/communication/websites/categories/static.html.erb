---
title: "<%= @about.name %>"
slug: "<%= @about.path %>"
<% if @about.parent %>
parent: "<%= @about.parent.path %>"
<% end %>
<% if @about.previous_links.any? %>
aliases:
<% @about.previous_links.each do |previous_link| %>
  - <%= previous_link.link %>
<% end %>
<% end %>
position: <%= @about.position %>
<%= render 'admin/communication/unsplash/static' %>
description: >
  <%= prepare_text_for_static @about.description %>
description_short: >
  <%= prepare_text_for_static @about.text %>
<%= render 'admin/communication/blocks/static', about: @about %>
---
