---
title: >
  <%= @teacher.to_s %>
slug: "<%= @teacher.slug %>"
first_name: "<%= @teacher.first_name %>"
last_name: "<%= @teacher.last_name %>"
phone: "<%= @teacher.phone %>"
email: "<%= @teacher.email %>"
<% if @teacher.education_programs.any? %>
programs:
  <% @teacher.education_programs.each do |program| %>
  - "<%= program.id %>"
  <% end %>
<% end %>
---
<%= prepare_for_github @teacher.biography, @teacher.university %>
