url: "<%= @about.permalinks.for_website(@website).current.first.path %>"
<% if @about&.slug %>
slug: "<%= @about.slug %>"
<% end %>
<%
past_permalinks = @about.permalinks.for_website(@website).not_current
if past_permalinks.any?
%>
aliases:
<% past_permalinks.each do |permalink| %>
  - <%= permalink.path %>
<% end %>
<% end %>
