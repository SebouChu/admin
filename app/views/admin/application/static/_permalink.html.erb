url: "<%= @about.current_permalink_in_website(@website)&.path %>"
<% if @about&.slug %>
slug: "<%= @about.slug %>"
<% end %>
<%
previous_permalinks_in_website = @about.previous_permalinks_in_website(@website)
if previous_permalinks_in_website.any?
%>
aliases:
<% previous_permalinks_in_website.each do |permalink| %>
  - <%= permalink.path %>
<% end %>
<% end %>
